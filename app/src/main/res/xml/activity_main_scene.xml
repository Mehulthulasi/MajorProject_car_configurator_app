<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="3000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="0"
               android:scaleX="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="0"
               android:scaleY="1.2" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="20"
               android:scaleX="1" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="20"
               android:scaleY="1" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="40"
               android:scaleX="0.9" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="40"
               android:scaleY="0.9" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="50"
               android:scaleX="0.8" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="50"
               android:scaleY="0.8"  />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="60"
               android:scaleX="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="60"
               android:scaleY="0.7" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="70"
               android:scaleX="1.6" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="70"
               android:scaleY="1.6" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="80"
               android:scaleX="2" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="80"
               android:scaleY="2" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="85"
               android:scaleY="5.5" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="85"
               android:scaleX="5.5" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="90"
               android:scaleY="10" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="90"
               android:scaleX="10" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="92"
               android:scaleY="15" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="92"
               android:scaleX="15" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="97"
               android:scaleX="30" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="97"
               android:scaleY="30" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="100"
               android:scaleX="50" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="100"
               android:scaleY="50" />


       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>